extern getwchar() char;
extern putwchar(c char) char;


fn getStrLn() {char} {
    let str = {char}();
    let loop = true;

    while loop {
        switch getwchar() {
            '\n': loop = false;
            c   : str = append(str, c);
        }
    }

    return str;
}


fn putStrLn(str {char}) i64 {
    let i = 0;
    while i < len(str) {
        putwchar(str[i]);
        i = i + 1;
    }

    putwchar('\n');
    return i;
}
        
        
let line = getStrLn();
print(putStrLn(line));
