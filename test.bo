extern getchar() i32;
extern putchar(c i32) i32;

fn getLine() {
	c := char(getchar());

	switch true {
		c < char(0): return;
		c == '\t'  : {
			putchar(i32(' '));
			putchar(i32(' '));
			putchar(i32(' '));
			putchar(i32(' '));
		}
		_ : {
			putchar(i32(c));
		}
	}

	getLine();
}
		
getLine();
