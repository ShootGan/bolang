module main

imports std/strings
imports std/io
imports lang/lexer
imports lang/parser 

fn main()
    let str = ""
    let loop = true

    while loop
        switch getStrLn()
            "\0" ..; loop = false
            s      ; str = str + s + "\n"

    let toks = :[Token]()
    loop = true

    while loop
        switch lexToken(str)
            (null, ss); loop = false
            (tok,  ss)
                str  = ss
                toks = append(toks, [tok])

