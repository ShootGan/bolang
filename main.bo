module main

imports lang/lexer
imports std/io

fn main()
    let str = ""

    let loop = true
    while loop
        switch getStrLn()
            "\0" ..; loop = false
            s      ; str = append(str, append(s, "\n"))


    loop = true
    while loop
        let (tok, ss) = lexToken(str)
        switch tok
            null; loop = false
            t
                print(t)
                str = ss
