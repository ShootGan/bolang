module vec2

type Vec2 (x:f64, y:f64)

fn +(a Vec2, b Vec2) Vec2
    return Vec2(a.x + b.x, a.y + b.y)

fn -(a Vec2, b Vec2) Vec2
    return Vec2(a.x - b.x, a.y - b.y)

fn *(a Vec2, b Vec2) Vec2
    return Vec2(a.x * b.x, a.y * b.y)

fn /(a Vec2, b Vec2) Vec2
    return Vec2(a.x / b.x, a.y / b.y)

fn ==(a Vec2, b Vec2) bool
    return a.x == b.x && a.y == b.y
