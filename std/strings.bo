module strings


fn isPrefix(pre string, str string) bool
    let i = 0
    while i < len(pre) 
        if i >= len(str) || pre[i] != str[i]
            return false 
        i = i + 1

    return true


fn strEqual(a string, b string) bool
    if len(a) != len(b)
        return false

    let i = 0
    while i < len(a)
        if a[i] != b[i]
            return false
        i = i + 1

    return true
