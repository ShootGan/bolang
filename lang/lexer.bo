module lexer

imports ../std/io
imports ../std/strings

type TokSym String
type TokInt i64

type Token {null, TokSym, TokInt}


fn parseLet(str String) (:Token, :String)
    let s = String("let")
    let t = &TokSym(s)

    if isPrefix(s, str)
        return ( Token(t), str[len(s)..] )

    return (Token(t), str)
